    "panels": [
      {
        "id": 1,
        "title": "RPS - Requests per Second (Last 5m)",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m])) by (method)",
            "legendFormat": "{{method}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "unit": "reqps",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "LAT - Latency Percentiles (P50/P95/P99)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "unit": "seconds",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 2,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true,
            "yaxis": "left"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "ERR - Error Rate by Status Code",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{status=~\"4..\"}[5m])) by (status)",
            "legendFormat": "{{status}} errors",
            "refId": "A"
          },
          {
            "expr": "sum(rate(http_requests_total{status=~\"5..\"}[5m])) by (status)",
            "legendFormat": "{{status}} errors",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "unit": "reqps",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "SAT - System Saturation",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m]) * 100",
            "legendFormat": "CPU %",
            "refId": "A"
          },
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "Memory %",
            "refId": "B"
          },
          {
            "expr": "(1 - (node_filesystem_avail_bytes{mountpoint=\"/\"} / node_filesystem_size_bytes{mountpoint=\"/\"})) * 100",
            "legendFormat": "Disk %",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        ],
        "thresholds": [
          {
            "value": 80,
            "colorMode": "warning",
            "op": "gt",
            "fill": true,
            "line": true,
            "yaxis": "left"
          },
          {
            "value": 90,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true,
            "yaxis": "left"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "mongodb_connections{state=\"active\"}",
            "legendFormat": "Active",
            "refId": "A"
          },
          {
            "expr": "mongodb_connections{state=\"available\"}",
            "legendFormat": "Available",
            "refId": "B"
          },
          {
            "expr": "mongodb_connections{state=\"current\"}",
            "legendFormat": "Current",
            "refId": "C"
          }
