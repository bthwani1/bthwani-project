{
  "cli": { "appVersionSource": "remote" },
  "build": {
    "production": {
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true
      },
      "ios": {
        "autoIncrement": "buildNumber"
      },
      "releaseChannel": "production"
    },
    "aab": {
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true
      }
    },
    "preview": {
      "android": { "buildType": "apk" }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "apk" }
    }
  }
}
